<!DOCTYPE html>
<html lang="pt-br">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Parfait - Encontre a roupa perfeita pra você</title>
    <meta name="keywords" content="HTML5 Template">
    <meta name="description" content="Parfait - Encontre a roupa perfeita pra você">
    <meta name="author" content="p-themes">
    <!-- Favicon
    <link rel="apple-touch-icon" sizes="180x180" href="~/assets/images/icons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/assets/images/icons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/assets/images/icons/favicon-16x16.png">
    <link rel="manifest" href="~/assets/images/icons/site.html">
    <link rel="mask-icon" href="~/assets/images/icons/safari-pinned-tab.svg" color="#666666">
    <link rel="shortcut icon" href="~/assets/images/icons/favicon.ico">-->
    <meta name="apple-mobile-web-app-title" content="Parfait">
    <meta name="application-name" content="Parfait">
    <meta name="msapplication-TileColor" content="#cc6666">
    <meta name="msapplication-config" content="~/assets/images/icons/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">
    <!-- Plugins CSS File -->
    <link rel="stylesheet" href="~/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/css/plugins/owl-carousel/owl.carousel.css">
    <link rel="stylesheet" href="~/assets/css/plugins/magnific-popup/magnific-popup.css">
    <!-- Main CSS File -->
    <link rel="stylesheet" href="~/assets/css/plugins/nouislider/nouislider.css">
    <link rel="stylesheet" href="~/assets/css/style.css">
    <link rel="stylesheet" href="~/assets/css/custom.style.css">
</head>
<body>
    <div class="overlay" hidden id="overlay-redirect" style="position:fixed;z-index:10; width:100%; height:100%; background-color:rgba(255, 255, 255, 0.72)">
        <div class="aviso" style="position: fixed;top: 50%; left: 50%; transform: translate(-50%, -50%);">
            <div class="spinner-grow" role="status">
                <span class="sr-only">Loading...</span>
            </div>
            <div class="spinner-grow" role="status">
                <span class="sr-only">Loading...</span>
            </div>
            <div class="spinner-grow" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
        <div style="position: fixed;top: 40%; left: 50%; transform: translate(-50%, -50%); text-align:center;">
            <div>
                <h5>Estamos redirecionando você até um atendente</h5>
                <p><strong>Sua posição na fila: 1</strong></p>
                <small>habilite o pop-up do navegador</small>
            </div>
        </div>
    </div>
    <div class="page-wrapper">
        <partial name="@(ViewBag.header??"_DefaultHeaderPartial")" />

        <main class="main">
            @RenderBody()
        </main><!-- End .main -->

        <partial name="_FooterPartial" />
    </div><!-- End .page-wrapper -->

    <button id="scroll-top" title="Ir para o topo"><i class="icon-arrow-up"></i></button>

    <partial name="_MenuMobilePartial" />

    <!-- Plugins JS File -->
    <script src="~/assets/js/jquery.min.js"></script>
    <script src="~/lib/jquery.appear/jquery.appear.min.js"></script>
    <script src="~/assets/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/jquery.lazyload/jquery.lazyload.min.js"></script>
    <script src="~/assets/js/jquery.hoverIntent.min.js"></script>
    <script src="~/assets/js/jquery.waypoints.min.js"></script>
    <script src="~/assets/js/superfish.min.js"></script>
    <script src="~/assets/js/owl.carousel.min.js"></script>
    <script src="~/assets/js/wNumb.js"></script>
    <script src="~/assets/js/jquery.magnific-popup.min.js"></script>
    <script src="~/assets/js/nouislider.min.js"></script>

    <!-- Main JS File -->
    <script src="~/assets/js/main.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
